{% extends 'base.html.twig' %}

{% block body %}
    <h1>Suiviticket</h1>

    <table>
        <tbody>
            <tr>
                <th>Date</th>
                <td>{% if suiviTicket.date %}{{ suiviTicket.date|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Remarque</th>
                <td>{{ suiviTicket.remarque }}</td>
            </tr>
            <tr>
                <th>Idsuiviticket</th>
                <td>{{ suiviTicket.idSuiviTicket }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('suiviticket_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('suiviticket_edit', { 'idSuiviTicket': suiviTicket.idSuiviTicket }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
